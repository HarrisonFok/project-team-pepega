{
  "name": "project-team-pepega",
  "version": "1.0.0",
  "description": "In-browser video editing app",
  "main": "server.js",
  "scripts": {
    "nodemon-debug": "npm install -g nodemon@debug",
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run nodemon-debug\" \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false && npm install --prefix client && npm run build --prefix client",
    "install-all": "npm install && cd client && npm install && cd .."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UTSCC09/project-team-pepega.git"
  },
  "author": "Martin Liang, Harrison Fok, Qi Lin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/UTSCC09/project-team-pepega/issues"
  },
  "homepage": "https://github.com/UTSCC09/project-team-pepega#readme",
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.0.20",
    "@ffprobe-installer/ffprobe": "^1.0.12",
    "@material-ui/core": "^4.9.5",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "config": "^3.3.0",
    "express": "^4.17.1",
    "fluent-ffmpeg": "^2.1.2",
    "form-data": "^3.0.0",
    "fs": "0.0.1-security",
    "gridfs-stream": "^1.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.2",
    "multer": "^1.4.2",
    "multer-gridfs-storage": "^4.0.2",
    "path": "^0.12.7",
    "video-react": "^0.14.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
